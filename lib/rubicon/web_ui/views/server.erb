<h1><%= server.name %></h1>

<table class="table table-striped table-hover table-border">
    <thead>
        <th>Rank</th>
        <th>Name</th>
        <th>Kills</th>
        <th>Deaths</th>
        <th>Score</th>
        <th>Ping</th>
        <th>Actions</th>
    </thead>
    <tbody>
        <% server.players.each do |name, player| %>
        <tr>
            <td><i class="sprite sprite-rank-<%=player.rank%>"></i></td>
            <td><%= player.name %></td>
            <td><%= player.kills %></td>
            <td><%= player.deaths %></td>
            <td><%= player.score %></td>
            <td><%= player.ping %></td>
            <td>actions</td>
        </tr>
        <% end %>
    </tbody>
</table>

<script src="/javascripts/server.js"></script>